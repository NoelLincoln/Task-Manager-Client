{
    "compilerOptions": {
      "jsx": "react",
      "composite": true,
      "skipLibCheck": true,
      "module": "ESNext",
      "moduleResolution": "bundler",
      "allowSyntheticDefaultImports": true,
      "target": "ES6",
      "outDir": "./dist",
      "rootDir": "./src"
    },
    "include": [
      "vite.config.ts",
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.json"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  